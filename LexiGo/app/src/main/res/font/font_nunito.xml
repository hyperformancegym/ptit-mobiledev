<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <!-- Light -->
    <font
        android:fontStyle="normal"
        android:fontWeight="300"
        android:font="@font/nunito_light"
        app:fontStyle="normal"
        app:fontWeight="300"
        app:font="@font/nunito_light"/>

    <!-- Regular -->
    <font
        android:fontStyle="normal"
        android:fontWeight="400"
        android:font="@font/nunito_regular"
        app:fontStyle="normal"
        app:fontWeight="400"
        app:font="@font/nunito_regular"/>

    <!-- Italic -->
    <font
        android:fontStyle="italic"
        android:fontWeight="400"
        android:font="@font/nunito_italic"
        app:fontStyle="italic"
        app:fontWeight="400"
        app:font="@font/nunito_italic"/>

    <!-- Medium -->
    <font
        android:fontStyle="normal"
        android:fontWeight="500"
        android:font="@font/nunito_medium"
        app:fontStyle="normal"
        app:fontWeight="500"
        app:font="@font/nunito_medium"/>

    <!-- SemiBold -->
    <font
        android:fontStyle="normal"
        android:fontWeight="600"
        android:font="@font/nunito_semibold"
        app:fontStyle="normal"
        app:fontWeight="600"
        app:font="@font/nunito_semibold"/>

    <!-- Bold -->
    <font
        android:fontStyle="normal"
        android:fontWeight="700"
        android:font="@font/nunito_bold"
        app:fontStyle="normal"
        app:fontWeight="700"
        app:font="@font/nunito_bold"/>

    <!-- ExtraBold -->
    <font
        android:fontStyle="normal"
        android:fontWeight="800"
        android:font="@font/nunito_extrabold"
        app:fontStyle="normal"
        app:fontWeight="800"
        app:font="@font/nunito_extrabold"/>

</font-family>
